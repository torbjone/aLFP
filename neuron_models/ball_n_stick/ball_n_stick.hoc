proc celldef() {
  topol()
  subsets()
}
create soma[1], dend[1]

proc topol() { local i
  connect dend[0](0), soma[0](1)
  basic_shape()
}
proc basic_shape() {
  soma {pt3dclear() pt3dadd(0, -10, 0, 10.) pt3dadd(0, 10, 0, 10.)}
  dend {pt3dclear() pt3dadd(0, 10, 0, 2.) pt3dadd(0, 1000, 0, 2.)}
}

objref all
proc subsets() { local i
  objref all
  all = new SectionList()
    soma all.append()
    dend all.append()

}
access soma
celldef()
